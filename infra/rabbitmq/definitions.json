{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "users": [
    {
      "name": "guest",
      "password": "guest",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "internal_cluster_id",
      "value": "rabbitmq-cluster-copilot-for-consensus"
    }
  ],
  "policies": [],
  "queues": [
    {
      "name": "archive.ingested",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "json.parsed",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "summarization.requested",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "summary.complete",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "exchanges": [
    {
      "name": "copilot.events",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "copilot.events",
      "vhost": "/",
      "destination": "archive.ingested",
      "destination_type": "queue",
      "routing_key": "archive.ingested",
      "arguments": {}
    },
    {
      "source": "copilot.events",
      "vhost": "/",
      "destination": "json.parsed",
      "destination_type": "queue",
      "routing_key": "json.parsed",
      "arguments": {}
    },
    {
      "source": "copilot.events",
      "vhost": "/",
      "destination": "summarization.requested",
      "destination_type": "queue",
      "routing_key": "summarization.requested",
      "arguments": {}
    },
    {
      "source": "copilot.events",
      "vhost": "/",
      "destination": "summary.complete",
      "destination_type": "queue",
      "routing_key": "summary.complete",
      "arguments": {}
    }
  ]
}
