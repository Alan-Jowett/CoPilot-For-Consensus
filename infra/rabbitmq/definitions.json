{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "users": [
    {
      "name": "guest",
      "password": "guest",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "internal_cluster_id",
      "value": "rabbitmq-cluster-copilot-for-consensus"
    }
  ],
  "policies": [],
  "queues": [
    {
      "name": "archive.ingested",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {},
      "_comment": "Consumed by parsing service"
    },
    {
      "name": "json.parsed",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {},
      "_comment": "Consumed by chunking service"
    },
    {
      "name": "summarization.requested",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {},
      "_comment": "Consumed by summarization service"
    },
    {
      "name": "summary.complete",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {},
      "_comment": "Consumed by reporting service"
    }
  ],
  "exchanges": [
    {
      "name": "copilot.events",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "copilot.events",
      "vhost": "/",
      "destination": "archive.ingested",
      "destination_type": "queue",
      "routing_key": "archive.ingested",
      "arguments": {},
      "_comment": "Parsing service consumes archive.ingested events"
    },
    {
      "source": "copilot.events",
      "vhost": "/",
      "destination": "json.parsed",
      "destination_type": "queue",
      "routing_key": "json.parsed",
      "arguments": {},
      "_comment": "Chunking service consumes json.parsed events"
    },
    {
      "source": "copilot.events",
      "vhost": "/",
      "destination": "summarization.requested",
      "destination_type": "queue",
      "routing_key": "summarization.requested",
      "arguments": {},
      "_comment": "Summarization service consumes summarization.requested events"
    },
    {
      "source": "copilot.events",
      "vhost": "/",
      "destination": "summary.complete",
      "destination_type": "queue",
      "routing_key": "summary.complete",
      "arguments": {},
      "_comment": "Reporting service consumes summary.complete events"
    }
  ]
}
